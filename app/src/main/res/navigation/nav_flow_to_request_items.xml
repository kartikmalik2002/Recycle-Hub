<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_flow_to_request_items"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.recyclehub.activities.fragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_selectQtyFragment"
            app:destination="@id/selectQtyFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectQtyFragment"
        android:name="com.example.recyclehub.activities.fragments.SelectQtyFragment"
        android:label="fragment_select_qty"
        tools:layout="@layout/fragment_select_qty" />
</navigation>